<Tk xmlns="Tkinter" title="Calculator" geometry="300x400" bg="#343845" resizable="false false">

    <Variable name="display"/>

    <ConfigClass name="yeet" width="3" command="insert" bg="#cbe6f7" fg="black" activebackground="white" activeforeground="black" font="Arial; 20"/>

    <Label justify="right" font="Arial;30" bg="#2c2f38" fg="white" textvariable="display">
        <pack fill="x" padx="5" pady="5"/>
    </Label>
    <Frame bg="#2c2f38">
        <Button command="clear" bg="#296d99" fg="white" activebackground="#59a0cf" activeforeground="white" font="Arial; 20">C<grid row="0" column="0" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">/<grid row="0" column="1" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">%<grid row="0" column="2" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">*<grid row="0" column="3" sticky="NESW" padx="5" pady="5"/></Button>

        <Button configclass="yeet">7<grid row="1" column="0" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">8<grid row="1" column="1" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">9<grid row="1" column="2" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">-<grid row="1" column="3" sticky="NESW" padx="5" pady="5"/></Button>

        <Button configclass="yeet">4<grid row="2" column="0" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">5<grid row="2" column="1" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">6<grid row="2" column="2" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">+<grid row="2" column="3" sticky="NESW" padx="5" pady="5"/></Button>

        <Button configclass="yeet">1<grid row="3" column="0" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">2<grid row="3" column="1" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">3<grid row="3" column="2" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">=<grid rowspan="2" row="3" column="3" sticky="NESW" padx="5" pady="5"/></Button>

        <Button configclass="yeet">0<grid columnspan="2" row="4" column="0" sticky="NESW" padx="5" pady="5"/></Button>
        <Button configclass="yeet">.<grid row="4" column="2" sticky="NESW" padx="5" pady="5"/></Button>

        <pack fill="both" pady="0 5" padx="5"/>
        <columnconfig index="0 1 2 3" weight="1"/>
        <rowconfig index="0 1 2 3 4" weight="1"/>
    </Frame>
</Tk>